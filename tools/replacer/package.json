{
  "name": "replacer",
  "displayName": "replacer",
  "description": "Markdown replacer to help with documentation writing",
  "publisher": "Inflectra Corporation",
  "icon": "replacer.png",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Inflectra/rapise-documentation.git"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "replacer.convertImage100",
        "title": "Convert Image 100"
      },
      {
        "command": "replacer.convertImage125",
        "title": "Convert Image 125"
      },
      {
        "command": "replacer.convertImage150",
        "title": "Convert Image 150"
      }      
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
